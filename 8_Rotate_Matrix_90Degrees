class HelloWorld {
    public static void main(String[] args) {
        int mat[][] = new int[][]{{1,2,3},{4,5,6},{7,8,9}};
        int len = mat.length;
        for(int i=0;i<len;i++)
        {
            for(int j=len-1;j>=0;j--)
            {
                System.out.print(mat[j][i]+" ");
            }
            System.out.println("");
        }
        
    }
}
